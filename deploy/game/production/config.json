{
    "game_service": {
        "instance_id": "game_service_0",
        "games_cap": 1000,
        "default_game_settings": {
            "time": 1500
        }
    },
    "redis": {
        "addr": "redis:6379"
    },
    "kafka": {
        "brokers": [
            "broker:9092"
        ],
        "group_id": "game_service_0"
    },
    "log": {
        "file": "./data/log/game_service.log",
        "verbose": true
    },
    "http": {
        "port": 8080
    },
    "grpc": {
        "port": 9090
    },
    "wsgateway_service_grpc": {
        "target": "wsgateway:9090"
    },
    "jwt_validator": {
        "public_key_path": "/app/jwt/public_key.pem"
    }
}